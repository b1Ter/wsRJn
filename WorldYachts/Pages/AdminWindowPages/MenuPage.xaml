<Page x:Class="WorldYachts.Pages.AdminWindowPages.MenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WorldYachts.Pages.AdminWindowPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenuPage">

    <Grid>
        <UniformGrid Columns="2">

            <Button x:Name="btnUser" Style="{StaticResource btnUsuallyMain}"
                    Content="Пользователи" Click="btnUser_Click"
                    Width="160" Height="30"/>

            <Button x:Name="btnAccessory" Style="{StaticResource btnUsuallyMain}"
                    Content="Акксесуары" Click="btnAccessory_Click"
                    Width="160" Height="30"/>

        </UniformGrid>
    </Grid>
</Page>
